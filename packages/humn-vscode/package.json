{
  "name": "humn-vscode",
  "displayName": "Humn Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "humn",
        "aliases": [
          "Humn",
          "humn"
        ],
        "extensions": [
          ".humn"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/humn-light.svg",
          "dark": "./icons/humn-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "humn",
        "scopeName": "source.humn",
        "path": "./syntaxes/humn.tmLanguage.json",
        "embeddedLanguages": {
          "text.html.basic": "html",
          "source.js": "javascript",
          "source.css": "css"
        }
      }
    ]
  }
}
